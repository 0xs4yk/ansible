[PARSER]
    Name        auth
    Format      regex
    Regex       ^(?<time>[^ ]+ [^ ]+ [^ ]+) (?<host>[^ ]+) (?<ident>.*?)\[(?<pid>[0-9]+)\].*?\(?(?<message>.*)$
    Time_Key    time
    Time_Format %b %d %H:%M:%S
    Time_Keep   On
    Types       pid:integer

[PARSER]
    Name        auditd
    Format      regex
    Regex       type=(?<type>[^ ]*) msg=audit\(\(?<timestamp>[^:]*\):(?<seq>[^:]*)\): (?<message>.*)$
    Time_Key    time
    Time_Format %s.%L
    Time_Keep   On
    Types       type:string seq:integer

[PARSER]
    Name        firewall
    Format      regex
    Regex       ^(?<time>[^ ]+ +[^ ]+ +[^ ]+) (?<host>[^ ]+) kernel: \[[^\]]*\] UFW BLOCK (?<direction>[^ ]+) (?<message>.*)$
    Time_Key    time
    Time_Format %b %d %H:%M:%S
    Time_Keep   On
    Types       direction:string

[PARSER]
    Name        syslog
    Format      regex
    Regex       ^\<(?<pri>[0-9]+)\>(?<time>[^ ]* {1,2}[^ ]* [^ ]*) (?<host>[^ ]*) (?<ident>[a-zA-Z0-9_\/\.\-]*)(?:\[(?<pid>[0-9]+)\])?(?:[^\:]*\:)? *(?<message>.*)$
    Time_Key    time
    Time_Format %b %d %H:%M:%S
    Time_Keep   On

[PARSER]
    Name        logfmt
    Format      logfmt

[PARSER]
    Name        json
    Format      json
    Time_Key    time
    Time_Format %iso8601
    Time_Keep   On